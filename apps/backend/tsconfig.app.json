{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"strict": true,
		"esModuleInterop": true,
		"strictNullChecks": false,
		"outDir": "../../dist/out-tsc",
		"module": "ES2022",
		"types": [
			"node",
			"express",
			"multer"
		],
		"target": "es2021"
	},
	"exclude": [
		"jest.config.ts",
		"src/**/*.spec.ts",
		"src/**/*.test.ts"
	],
	"include": [
		"src/**/*.ts"
	]
}
